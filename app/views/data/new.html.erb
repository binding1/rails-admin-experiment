<h1>New Data</h1>

<%= form_with model: @data_path do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :url %><br>
    <%= form.text_area :url%>
  </div>

  <div>
    <%= form.label :number_of_clicks %><br>
    <%= form.text_area :number_of_clicks%>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
